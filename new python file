def insertion_sort(array):
    for i in range(1,len(array)):
        key=array[i]
        j=i-1
        while j>=0 and key<array[j]:
            array[j+1]=array[j]
            j=j-1
        array[j+1]=key
array=[10,9,4,8,2]
print("Unsorted Array:",array)
insertion_sort(array)
print("Sorted Array:",array)
